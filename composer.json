{
    "name": "dspacelabs/ecwid",
    "description": "Ecwid PHP Client",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/dSpaceLabs/Ecwid",
    "authors": [
        {
            "name": "Joshua Estes",
            "homepage": "http://joshuaestes.me"
        }
    ],
    "support": {
